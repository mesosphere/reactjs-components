  "author": {
    "name": "Mesosphere Frontend Team",
    "email": "web-public@mesosphere.com",
    "url": "http://www.mesosphere.com"
  },
  "engines": {
    "node": "4.4.x",
    "npm": "3.9.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mesosphere/reactjs-components"
  },
  "license": "Apache-2.0",
  "files": [
    "main.js",
    "lib"
  ],
  "main": "main.js",
  "dependencies": {
    "classnames": "2.2.5",
    "lodash.throttle": "4.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.23.0",
    "browser-sync": "2.18.12",
    "cnvs": "1.1.x",
    "envify": "4.0.0",
    "eslint": "4.1.1",
    "eslint-config-dcos": "github:dcos/javascript#v0.2.1",
    "eslint-plugin-compat": "1.0.4",
    "eslint-plugin-destructuring": "2.1.0",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "6.0.0",
    "eslint-plugin-jsx-max-len": "1.0.0",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "glob": "7.1.2",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-connect": "5.0.0",
    "gulp-eslint": "4.0.0",
    "gulp-less": "3.3.2",
    "gulp-minify-css": "1.2.4",
    "gulp-replace": "0.6.1",
    "gulp-sourcemaps": "2.6.0",
    "gulp-stylelint": "3.9.0",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "jest-cli": "20.0.4",
    "less-color-lighten": "0.0.1",
    "node-libs-browser": "2.0.0",
    "prettier": "1.4.4",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.6.0",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-gemini-scrollbar": "~2.3.2",
    "source-map-loader": "0.2.1",
    "stylelint": "7.12.0",
    "stylelint-config-dcos": "0.0.3",
    "stylelint-config-standard": "16.0.0",
    "stylelint-webpack-plugin": "0.7.1",
    "transform-loader": "0.2.4",
    "webpack": "3.0.0"
  },
  "peerDependencies": {
    "cnvs": "1.1.x",
    "react": "0.14.x || 15.0.x || 15.4.x",
    "react-addons-css-transition-group": "0.14.x || 15.0.x || 15.4.x",
    "react-gemini-scrollbar": "2.1.x"
  },
  "scripts": {
    "clean": "rm -rf dist docs/dist lib",
    "dist": "npm run clean && NODE_ENV='production' ./node_modules/.bin/gulp docs:dist",
    "predist-src": "rm -rf ./lib",
    "dist-src": "cp -r ./src ./lib && find ./lib -name '__tests__' -type d -exec rm -r '{}' + && ./node_modules/.bin/babel lib --out-dir lib",
    "livereload": "NODE_ENV='development' ./node_modules/.bin/gulp docs:livereload",
    "test": "NODE_PATH=node_modules jest --config=jest/config.json --no-cache",
    "release": "npm version -m 'Version %s'",
    "release:major": "npm run release -- major",
    "release:minor": "npm run release -- minor",
    "release:patch": "npm run release -- patch",
    "release:premajor": "npm run release -- premajor",
    "release:preminor": "npm run release -- preminor",
    "release:prepatch": "npm run release -- prepatch",
    "release:prerelease": "npm run release -- prerelease",
    "release:beta": "npm run release -- prerelease",
    "lint": "npm run stylelint && npm run eslint",
    "eslint": "eslint --quiet src/**/*.js docs/src/**/*.js",
    "stylelint": "stylelint src/**/*.less  docs/src/**/*.less",
    "preversion": "npm run clean && npm test && npm run lint && npm run dist-src",
    "postversion": "git push --tags && npm publish ./ --tag version-$npm_package_version && git push && echo \"Successfully released $npm_package_version\""
  }
}
