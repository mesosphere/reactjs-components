{
  "name": "mesosphere-ui-components",
  "version": "0.0.1",
  "description": "React UI Components by Mesosphere",
  "author": "Mesosphere Web Team",
  "license": "Apache-2.0",
  "dependencies": {
    "babel": "^5.6.23",
    "body-parser": "^1.13.2",
    "canvas-ui-system": "^0.1.2",
    "react": "^0.13.3",
    "react-tools": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^5.7.3",
    "babel-eslint": "^3.1.23",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.7.13",
    "canvas-ui-system": "^0.1.2",
    "classnames": "^2.1.3",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^2.6.4",
    "glob": "^5.0.14",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^0.15.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.0",
    "gulp-replace": "^0.5.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.4.15",
    "node-libs-browser": "^0.5.2",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.10.1"
  },
  "scripts": {
    "clean": "rm -rf dist docs/dist",
    "dist": "npm run clean && NODE_ENV='production' ./node_modules/.bin/gulp docs:dist",
    "livereload": "NODE_ENV='development' ./node_modules/.bin/gulp docs:livereload",
    "npm-publish": "npm publish ./",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "setupTestFrameworkScriptFile": "jest/setupTestFramework.js",
    "setupEnvScriptFile": "jest/setupEnv.js",
    "modulePathIgnorePatterns": [
      "/tmp/",
      "/node_modules/",
      "/.module-cache/"
    ],
    "unmockedModulePathPatterns": [
      "classnames",
      "react",
      "react-router",
      "react-tools"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
